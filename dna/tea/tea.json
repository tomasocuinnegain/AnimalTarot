{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "type",
    "grade"
  ],
  "properties": {
    "type": {
      "$id": "#/properties/type",
      "type": "string",
      "title": "The Type Schema",
      "default": "",
      "examples": [
        "Black"
      ],
      "pattern": "^(.*)$"
    },
    "grade": {
      "$id": "#/properties/grade",
      "type": "string",
      "title": "The Grade Schema",
      "default": "",
      "examples": [
        "SFTGFOP1"
      ],
      "pattern": "^(.*)$"
    }
  }
}